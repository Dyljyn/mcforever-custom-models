{
  "model": {
    "type": "select",
    "property": "component",
    "component": "custom_name",
    "cases": [
      {
        "when": "Goonsip",
        "model": {
          "type": "model",
          "model": "item/book/goonsip"
        }
      },
      {
        "when": "Goonsip #1",
        "model": {
          "type": "model",
          "model": "item/book/goonsip-diamond"
        }
      },
      {
        "when": "Goonsip #2",
        "model": {
          "type": "model",
          "model": "item/book/goonsip-gold"
        }
      },
      {
        "when": "Goonsip #3",
        "model": {
          "type": "model",
          "model": "item/book/goonsip-iron"
        }
      }
    ],
    "fallback": {
      "type": "select",
      "property": "custom_model_data",
      "cases": [
        {
          "when": "post-id-card",
          "model": {
            "type": "model",
            "model": "item/book/post-id-card"
          }
        }
      ],
      "fallback": {
        "type": "model",
        "model": "item/book"
      }
    }
  }
}

